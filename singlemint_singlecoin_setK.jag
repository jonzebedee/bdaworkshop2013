# JAGS model specification begins ...
model {
    # Likelihood:
    for (i in 1:nFlips) {
        y[i] ~ dbern( theta[1] )
    }

    # Prior distribution:
    mu ~ dbeta( priorA, priorB )I(0.0001, 0.9999)
    theta[1] ~ dbeta( a, b)

    a <- mu * K
    b <- (1 - mu) * K

    priorA <- 2
    priorB <- 2
}
# ... JAGS model specification ends.
